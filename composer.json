{
	"name": "flarum-lang/arabic",
	"type": "flarum-extension",
	"description": "Arabic language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/arabic",
	"keywords": [
		"locale",
		"flarum",
		"ar-locale"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Neeru Gupta",
			"email": "neeru.gupta@yahoo.com",
			"homepage": "https://github.com/neerugupta/"
		}
	],
	"support": {
		"issues": "https://github.com/flarum-lang/arabic/issues",
		"source": "https://github.com/flarum-lang/arabic"
	},
	"require": {
		"flarum/core": "^1.3"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
